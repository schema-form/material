{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Address",
  "properties": {
    "postalCode": {
      "title": "Zip/Postal code",
      "type": "number"
    },
    "streetName": {
      "title": "Street",
      "type": "string"
    },
    "streetType": {
      "title": "Type",
      "enum": ["Street", "Avenue", "Boulevard"]
    }
  },
  "required": ["postalCode", "streetName", "streetType"],
  "additionalProperties": {
    "title": "",
    "type": "string"
  },
  "default": {
    "postalCode": 1600,
    "streetName": "Pennsylvania",
    "streetType": "Avenue"
  }
}
