{
	"$schema": "http://json-schema.org/draft-07/schema",
  "type": "array",
  "title": "Address",
  "items": [
    {
      "title": "Zip/Postal code",
      "type": "number"
    },
    {
      "title": "State/Province",
      "type": "string"
    },
    {
      "enum": ["Street", "Avenue", "Boulevard"]
    },
    {
      "enum": ["NW", "NE", "SW", "SE"]
    }
  ],
  "additionalItems": {
    "type": "string"
  },
  "default": [1600, "Pennsylvania", "Avenue", "NW", "Washington"]
}
