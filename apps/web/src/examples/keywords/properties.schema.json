{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Shipping Address",
  "properties": {
    "street": {
      "type": "string",
      "title": "Street"
    },
    "city": {
      "type": "string",
      "title": "City"
    },
    "state": {
      "type": "string",
      "title": "State/Province"
    },
    "zip": {
      "type": "number",
      "title": "ZIP/Postal code"
    },
    "country": {
      "type": "string",
      "title": "Country"
    }
  },
  "required": ["street", "city", "state", "zip", "country"]
}
