{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Change password",
  "properties": {
    "oldPassword": {
      "title": "Old password",
      "type": "string",
      "format": "password",
      "const": "12345",
      "x-errorMessage": {
        "const": "enter your current password"
      }
    },
    "newPassword": {
      "title": "New password",
      "type": "string",
      "format": "password",
      "minLength": 8
    }
  }
}
