{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "States",
  "properties": {
    "normal": {
      "type": "string",
      "title": "Normal",
      "description": "Helper text",
      "contentMediaType": "text/x-markdown",
      "default": "Example of **emphasize** and *italicized* text"
    },
    "disabled": {
      "type": "string",
      "title": "Disabled",
      "description": "Helper text",
      "contentMediaType": "text/x-markdown",
      "readOnly": true,
      "default": "Example of **emphasize** and *italicized* text"
    },
    "invalid": {
      "type": "string",
      "title": "Invalid",
      "description": "Helper text",
      "contentMediaType": "text/x-markdown",
      "minLength": 100,
      "default": "Example of **emphasize** and *italicized* text"
    }
  }
}
