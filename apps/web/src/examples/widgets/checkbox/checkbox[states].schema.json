{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "States",
  "properties": {
    "checked": {
      "type": "boolean",
      "title": "Checked",
      "default": true
    },
    "unchecked": {
      "type": "boolean",
      "title": "Unchecked",
      "default": false
    },
    "intermediate": {
      "type": "boolean",
      "title": "Intermediate"
    },
    "disabled": {
      "type": "boolean",
      "title": "Disabled",
      "default": true,
      "readOnly": true
    },
    "invalid": {
      "type": "boolean",
      "title": "Invalid",
      "default": false,
      "const": true
    }
  }
}
