{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "Examples",
  "properties": {
    "fontSize": {
      "title": "Font size",
      "description": "Adjust the font size",
      "type": "integer",
      "minimum": 8,
      "maximum": 24,
      "multipleOf": 2,
      "default": 14
    },
    "quantity": {
      "title": "Quantity",
      "description": "Enter a quantity between 1 and 100.",
      "type": "integer",
      "minimum": 1,
      "maximum": 100,
      "multipleOf": 1,
      "default": 1
    }
  }
}
