{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Персональные данные",
	"properties": {
		"fullName": {
			"type": "string",
			"title": "ФИО"
		},
		"gender": {
			"title": "Пол",
			"enum": ["мужской", "женский"],
			"default": ""
		},
		"birthdate": {
			"title": "Дата рождения",
			"type": "string",
			"format": "date"
		}
	},
	"required": ["fullName", "gender", "birthdate"]
}
